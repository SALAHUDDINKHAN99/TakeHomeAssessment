{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "THAssessment-adf"
		},
		"ls_adls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls'"
		},
		"ls_adls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://thassessmentsa.dfs.core.windows.net/"
		},
		"ls_blob_sasUri": {
			"type": "secureString",
			"metadata": "Secure string for 'sasUri' of 'ls_blob'"
		},
		"ls_databricks_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0410-200009-9p8temjy"
		},
		"ls_keyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://THAKeyVault.vault.azure.net/"
		},
		"ls_mySql_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "@{linkedService().Server_name}"
		},
		"ls_mySql_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "@{linkedService().Database_name}"
		},
		"ls_mySql_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "@{linkedService().User_name}"
		},
		"ls_snowflake_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "@{linkedService().Database}"
		},
		"ls_snowflake_properties_typeProperties_role": {
			"type": "string",
			"defaultValue": "@{linkedService().Role}"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "THAssessment-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adls_accountKey": {
						"value": "[parameters('ls_adls_accountKey')]"
					},
					"ls_adls_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_properties_typeProperties_url')]"
					},
					"ls_blob_sasUri": {
						"value": "[parameters('ls_blob_sasUri')]"
					},
					"ls_databricks_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_databricks_properties_typeProperties_existingClusterId')]"
					},
					"ls_keyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_keyVault_properties_typeProperties_baseUrl')]"
					},
					"ls_mySql_properties_typeProperties_server": {
						"value": "[parameters('ls_mySql_properties_typeProperties_server')]"
					},
					"ls_mySql_properties_typeProperties_database": {
						"value": "[parameters('ls_mySql_properties_typeProperties_database')]"
					},
					"ls_mySql_properties_typeProperties_username": {
						"value": "[parameters('ls_mySql_properties_typeProperties_username')]"
					},
					"ls_snowflake_properties_typeProperties_database": {
						"value": "[parameters('ls_snowflake_properties_typeProperties_database')]"
					},
					"ls_snowflake_properties_typeProperties_role": {
						"value": "[parameters('ls_snowflake_properties_typeProperties_role')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}